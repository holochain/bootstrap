{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "esnext",
    "module": "commonjs",
    "sourceMap": true,
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["@cloudflare/workers-types/2022-08-04"]
  },
  "include": ["./src/*.ts", "./src/**/*.ts"],
  "exclude": ["node_modules/", "dist/"]
}

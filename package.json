{
  "name": "holochain-bootstrap",
  "version": "1.0.0",
  "description": "holochain bootstrap cloudflare worker",
  "main": "dist/worker.js",
  "scripts": {
    "start": "node --experimental-vm-modules ./scripts/run-mininet.mjs",
    "rust:fmt": "cd rust && cargo fmt -- --check",
    "rust:clippy": "cd rust && cargo clippy",
    "rust:test": "cd rust && cargo test",
    "rust:build": "node ./scripts/run-build-worker.cjs",
    "build": "npm run rust:build && webpack",
    "dev": "NODE_ENV=development npm run build",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "test:rust": "npm run rust:fmt && npm run rust:clippy && npm run rust:test",
    "test:unit": "npx ts-mocha 'test/**/*.ts'",
    "test:integration": "webpack -c test-webpack.config.cjs && node --experimental-vm-modules ./scripts/run-integration-test.mjs",
    "test": "npm run test:rust && npm run format:check && npm run test:unit && npm run test:integration"
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "type": "module",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "wrangler": "^2.6.2",
    "@types/chai": "^4.3.0",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.10",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "chai": "^4.3.4",
    "copy-webpack-plugin": "^10.2.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "miniflare": "^2.11.0",
    "mocha": "^10.2.0",
    "node-fetch": "^3.3.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "service-worker-mock": "^2.0.5",
    "ts-loader": "^9.4.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "base64-js": "^1.5.1",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "lodash": "^4.17.21",
    "tweetnacl": "^1.0.3"
  }
}

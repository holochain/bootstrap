{
  "compilerOptions": {
    "outDir": "../transpiled",
    "target": "esnext",
    "module": "commonjs",
    "sourceMap": true,
    "experimentalDecorators": true,
    "lib": ["esnext"],
    "moduleResolution": "node",
    "typeRoots": ["./type-fixes"],
    "types": [
      "@cloudflare/workers-types",
      "@types/mocha",
      "@types/service-worker-mock"
    ]
  },
  "include": [
    "./*.ts",
    "./**/*.ts"
  ]
}
